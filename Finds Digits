#include<iostream>
#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t,n,r,s;
    cin>>t;
    while(t--)
    {
        
        int count=0;
        cin>>n;
        s=n;
        while(n>0)
        {
            r=n%10;
            if(r!=0)
            {
            if(s%r==0)
            {
                count++;
            }}
            
            n=n/10;
        }
        cout<<count<<endl;
    }
    return 0;
}
