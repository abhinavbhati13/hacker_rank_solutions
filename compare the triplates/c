#include<stdio.h>
int main()
{
    int i1,j1,k1;
    int i2,j2,k2;
    int A=0,B=0;
   
   scanf("%d",&i1);
   scanf("%d",&j1);
   scanf("%d",&k1);
   scanf("%d",&i2);
   scanf("%d",&j2);
   scanf("%d",&k2);
   if(i1>i2)
   A=A+1;
   else if (i2>i1) {
   B=B+1;
   }
   if(j1>j2)
   A=A+1;
   else if(j2>j1) {
   B=B+1;
   }
   if(k1>k2)
   A=A+1;
   else if(k2>k1){
   B=B+1;
   }
   printf("%d %d",A,B);
}
