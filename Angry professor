#include<iostream>
#include<vector>
using namespace std;
int main(){
   int t,j,k,i,n;
   cin>>t;
        




for(j=0;j<t;j++)
{
    cin>>n;
    cin>>k;
 vector<int> a(n);     
 
    for(i=0;i<n;i++)
{
    cin>>a[i];
}
    int i,count,j;
    count=0;

for(i=0;i<n;i++)
{
    if(a[i]<=0)
    {
        count++;
    }
}


if(count>=k)
{
    cout <<"NO"<<endl;
}
else 
cout <<"YES"<<endl;
}
 
}
