#include<stdio.h>
#include<math.h>
int main()
{
  unsigned long long int i,n,a[1000000],max,count=0;
    scanf("%llu",&n);
    for(i=0;i<n;i++)
    {
        scanf("%llu",&a[i]);
    }
    max=a[0];
    for(i=0;i<n;i++){
    if(a[i]>max)
    {
        max=a[i];
    }}
    for(i=0;i<n;i++)
    {
        if(max==a[i])
        count++;
    }
    printf("%llu",count);
    
    
    
}
